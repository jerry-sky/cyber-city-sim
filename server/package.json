{
    "name": "server",
    "version": "1.0.0",
    "description": "",
    "main": "dist/src/index.js",
    "scripts": {
        "clean": "rm -rf dist/*",
        "test": "jest --coverage --coverage-reports=text",
        "lint": "npx eslint ./src/**/* --ext .js,.jsx,.ts,.tsx",
        "prebuild": "npm run lint",
        "build": "npx tsc && cp ./.env ./dist/",
        "prestart": "npm run build",
        "start": "node .",
        "dev": "nodemon --watch \"./src/**/*\" -e ts,json --exec \"ts-node ./src/index\"",
        "dev-vanilla": "nodemon --watch \"./src/**/*\" -e ts,json --exec \"npm run start\""
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "secure-password": "^4.0.0",
        "session-file-store": "^1.5.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.8",
        "@types/express-session": "^1.17.0",
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.6",
        "@types/secure-password": "^3.1.0",
        "@types/session-file-store": "^1.2.1",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "eslint": "^7.13.0",
        "eslint-plugin-prettier": "^3.1.4",
        "install": "^0.13.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.6",
        "prettier": "2.1.2",
        "ts-jest": "^26.4.3",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.5"
    }
}
